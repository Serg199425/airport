<div class="panel panel-default">
  <div class="panel-heading table-title">Latest Flights</div>
  <table class="table">
    <thead>
      <tr>
        <th>ID</th>
        <th>Airplane Model</th>
        <th>Airplane Registration Number</th>
        <th>Takes Off Start Time</th>
        <th>Takes Off End Time</th>
        <th>Status</th>
      </tr>
    </thead>
    <tbody id="flights_container">
      <%= render partial: "flights_table_item", collection: @flights, as: :flight %>
    </tbody>
  </table>
</div>
<%= will_paginate @flights, renderer: BootstrapPagination::Rails %>